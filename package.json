{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Scott Hamilton <scott.hamilton388@gmail.com>",
  "license": "MIT",
  "description": "Basic React Boilerplate",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swhamilton/react-boilerplate.git"
  },
  "keywords": ["Node", "React"],
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  }
}
